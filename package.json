{
  "name": "react-vite-tailwind",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "dev": "vite",
    "build": "tsc && vite build && NODE_ENV=production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "generator": "plop --plopfile ./.generators/propfile.cjs",
    "serve": "vite preview",
    "start": "vite preview"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^40.2.0",
    "@ckeditor/ckeditor5-react": "^6.2.0",
    "@shopify/polaris": "12.6.0",
    "@shopify/polaris-icons": "8.1.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "draft-js": "^0.11.7",
    "i18next": "^23.7.7",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-currency-input-field": "^3.6.14",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^13.5.0",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^5.3.1",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "sweetalert2": "^11.10.1",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@types/react-input-mask": "^3.0.5",
    "@types/react-redux": "^7.1.32",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "plop": "^4.0.0",
    "postcss-cli": "^11.0.0",
    "sass": "^1.69.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
